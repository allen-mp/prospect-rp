			}
		}
	}
	return rankname;
}

SendGroupMessage(group, color, string2[])
{
	foreach( new i: Player )
	{
		if(PlayerInfo[i][pGroup] == group)
		{
			SendClientMessage(i, color, string2);
		}
	}
	return 1;
}

