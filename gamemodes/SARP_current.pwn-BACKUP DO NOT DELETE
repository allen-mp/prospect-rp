ABroadCast(color, const string2[], level)
{
	foreach( new i: Player )
	{
		if(IsPlayerConnected(i))
		{
			if(PlayerInfo[i][pAdmin] >= level)
			{
				SendClientMessage(i, color, string2);
			}
		}
	}
	return 1;
}
