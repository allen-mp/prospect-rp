				   FamilyInfo[idx][FamilyCodeine],
				   FamilyInfo[idx][FamilyLean],
				   FamilyInfo[idx][FamilyLeaderHide],
				   FamilyInfo[idx][fXP]);
			fwrite(file, coordsstring);
