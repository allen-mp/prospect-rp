	new hitmanStr[1500];

	format(hitmanStr, sizeof(hitmanStr),
		"Desert Eagle\t$2,500\n\
		Suppressed Pistol\t$5,000\n\
		{C40000}----------\t----------{FFFFFF}\n\
		M4\t$12,500\n\
