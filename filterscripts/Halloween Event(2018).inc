public OnDialogResponse(playerid, dialogid, response, listitem, inputtext[])
{
    if(dialogid == DIALOG_PICKY1)
    {
        if(response)
        {
			ShowPlayerDialog(playerid, DIALOG_PICKY2, DIALOG_STYLE_MSGBOX, "Mrs. Picky Puddle", "Thanks for helping me!\n Collect some water from the dirty lake infront of me and go to the Empty Field, The Farm. ", "", "OK");
			PlayerInEvent[playerid] = 1;
        }
        return 1;
    }
    if(dialogid == DIALOG_CHERRY1)
    {
        if(response)
        {
			GotPoison[playerid] = 1;
			SendClientMessage(playerid, COLOR_GREY, "You've bought some poison, (/addpoison) to add it in pumpkin.");
        }
        return 1;
    }
    if(dialogid == DIALOG_BUMP)
    {
        if(response)
        {
		    new string1[255];
			GotKeysLock[playerid] = 1;
		    PumpkinP[playerid] = 0;
		    ProxDetector(30.0, playerid, "Mr Bumpkin Floras ate the pumpkin and died. (( Bumpkin Floras ))", COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
            format(string1, sizeof(string1), "* %s moves their hands around Bumpkin's body and searches for the locker keys.", PlayerICName(playerid));
            ProxDetector(30.0, playerid, string1, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
            SendClientMessage(playerid, COLOR_GREY, "You've got the locker keys, return to Mrs. Picky Puddle.");
        }
        return 1;
    }
    if(dialogid == DIALOG_PICKY3)
    {
        if(response)
        {
		    GotKeysLock[playerid] = 0;
			GiveMoney(playerid, 50000);  // temp prize
			PlayerInfo[playerid][pTikis] += 10;
        }
        return 1;
    }


	return 1;//
}