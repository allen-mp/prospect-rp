forward PumpkinTime(playerid);
public PumpkinTime(playerid)
{
   PumpkinGot[playerid] = 1;
   TogglePlayerControllable(playerid, 1);

   new string1[255];
   format(string1, sizeof(string1), "* %s crouches down and picks up the pumpkin.", PlayerICName(playerid));
   ProxDetector(30.0, playerid, string1, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
   SendClientMessage(playerid, COLOR_GREY, "You got the pumpkin finally! Go to Mr. Cherry who lives at the Palomino Spooky Church and buy some poison.");
   return 1;
}

stock SavePlayerAccount(playerid)
{
  new name[MAX_PLAYER_NAME], file[256];
  GetPlayerName(playerid, name, sizeof(name));
  format(file, sizeof(file), PATH, name);
  {
    dini_IntSet(file, "EventStats", PlayerInEvent[playerid]);
  }
  return 1;
}

public OnPlayerDisconnect(playerid, reason)
{
    HGotWater[playerid] = 0;
	PumpkinGot[playerid] = 0;
	GotPoison[playerid] = 0;
	PumpkinP[playerid] = 0;
	GotKeysLock[playerid] = 0;
	PlayerInEvent[playerid] = 0;
	return 1;
}


//Commands;
CMD:halloweenhelp(playerid)
{
  SendClientMessage(playerid, COLOR_GREEN, "~~~~~~~~~~~Halloween Event Help~~~~~~~~~~");
  SendClientMessage(playerid, COLOR_GREY, "/halloweenjoin, /addpoison");
  return 1;
}

CMD:halloweenjoin(playerid)
{
  if(PlayerInEvent[playerid] == 255) return SendClientMessage(playerid, COLOR_GREY, "You're already in event or you've already played it.");
  SendClientMessage(playerid, COLOR_GREY, "Go to Dirty Lake at Back O Beyond and talk to Mrs. Picky Puddle");
  return 1;
}

CMD:addpoison(playerid)
{
  new string1[255];
  if(GotPoison[playerid] == 1 && PumpkinGot[playerid] == 1)
  {
	 GotPoison[playerid] = 0;
	 PumpkinGot[playerid] = 0;
	 PumpkinP[playerid] = 1;
	 format(string1, sizeof(string1), "* %s adds some poison to the pumpkin secretly.", PlayerICName(playerid)); // To be changed to PlayerICName
     ProxDetector(30.0, playerid, string1, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
	 SendClientMessage(playerid, COLOR_GREY, "Great! You've added some poison to the pumpkin now go and feed it to Mr. Bumpkin Floras.");
	 SendClientMessage(playerid, COLOR_GREY, "Mr. Bumpkin Floras lives at Cursed Forest of Chiliad.");
  }
  return 1;
}

